<p id="notice"><%= notice %></p>

<h1>Listing Clinics</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Eps asociada</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <div><% @clinics.each do |clinic| %></div>
      <tr>
        <td><%= clinic.name %></td>
        <td><%= clinic.address %></td>
        <td><%= clinic.eps_asociada %></td>
        <td><%= link_to 'Show', clinic %></td>
        <td><%= link_to 'Edit', edit_clinic_path(clinic) %></td>
        <td><%= link_to 'Destroy', clinic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

