<h1>Listing Users</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Message</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.lastname %></td>
         <td><%= user.email %></td>
         <td><%= user.pais %></td>
      </tr>
    <% end %>
  </tbody>
</table>