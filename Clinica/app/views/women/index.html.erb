<p id="notice"><%= notice %></p>

<h1>Listing Women</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Surname</th>
      <th>Lastname</th>
      <th>Secondlastname</th>
      <th>Documento</th>
      <th>Fecha de nacimiento</th>
      <th>Estado civil</th>
      <th>Numero de hijos</th>
      <th>Numero de abortos</th>
      <th>Problemas de embarazo</th>
      <th>Cancer de ovarios</th>
      <th>Cancer de mamas</th>
      <th>Algun otro cancer</th>
      <th>Hipertencion</th>
      <th>Diabetes</th>
      <th>Alzheimer</th>
      <th>Grupo sanguineo</th>
      <th>Otra</th>
      <th>Enfermedades</th>
      <th>Grupo sanguineo</th>
      <th>Enfermedades o tratamientos actitudes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @women.each do |woman| %>
      <tr>
        <td><%= woman.name %></td>
        <td><%= woman.surname %></td>
        <td><%= woman.lastname %></td>
        <td><%= woman.secondlastname %></td>
        <td><%= woman.documento_id %></td>
        <td><%= woman.fecha_de_nacimiento %></td>
        <td><%= woman.estado_civil %></td>
        <td><%= woman.numero_de_hijos %></td>
        <td><%= woman.numero_de_abortos %></td>
        <td><%= woman.problemas_de_embarazo %></td>
        <td><%= woman.cancer_de_ovarios %></td>
        <td><%= woman.cancer_de_mamas %></td>
        <td><%= woman.algun_otro_cancer %></td>
        <td><%= woman.hipertencion %></td>
        <td><%= woman.diabetes %></td>
        <td><%= woman.alzheimer %></td>
        <td><%= woman.grupo_sanguineo %></td>
        <td><%= woman.otra %></td>
        <td><%= woman.enfermedades %></td>
        <td><%= woman.grupo_sanguineo %></td>
        <td><%= woman.enfermedades_o_tratamientos_actitudes %></td>
        <td><%= link_to 'Show', woman %></td>
        <td><%= link_to 'Edit', edit_woman_path(woman) %></td>
        <td><%= link_to 'Destroy', woman, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

