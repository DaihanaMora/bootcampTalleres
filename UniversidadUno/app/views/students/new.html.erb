<h1>Create student</h1>

<%=link_to "ver todos students", students_path%>

<%= form_for @student do |f| %>
  
<p>
     <%= f.label :nombre %><br>
     <%= f.text_field :nombre %><br>
     <%= f.label :apellido %><br>
     <%= f.text_field :apellido %><br>
     <%= f.label :identificacion %><br>
     <%= f.text_field :identificacion %><br>
     <%= f.label :email %><br>
     <%= f.text_field :email %><br>
 </p>

 
 
  <p>
    <%= f.submit %>
  </p>
<% end %>